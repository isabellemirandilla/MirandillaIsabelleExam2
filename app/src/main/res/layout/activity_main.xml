<?xml version="1.0" encoding="utf-8"?>
<RelativeLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    tools:context=".MainActivity">

    <View
        android:id="@+id/view"
        android:layout_width="316dp"
        android:layout_height="137dp"
        android:layout_alignParentTop="true"
        android:layout_centerHorizontal="true"
        android:layout_marginTop="85dp"
        android:background="#F5F5DC"
        android:visibility="visible" />

    <View
        android:id="@+id/view2"
        android:layout_width="316dp"
        android:layout_height="137dp"
        android:layout_centerInParent="true"
        android:background="#F5F5DC"
        android:visibility="visible" />

    <View
        android:id="@+id/view3"
        android:layout_width="316dp"
        android:layout_height="137dp"
        android:layout_alignParentBottom="true"
        android:layout_centerHorizontal="true"
        android:layout_marginBottom="97dp"
        android:background="#F5F5DC" />

    <Button
        android:id="@+id/MapsActivity1"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_alignParentBottom="true"
        android:layout_alignParentEnd="true"
        android:layout_marginBottom="17dp"
        android:layout_marginEnd="69dp"
        android:onClick="process"
        android:text="Map" />

    <TextView
        android:id="@+id/textView2"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_above="@+id/textView4"
        android:layout_alignStart="@+id/view"
        android:text="Clark Kent"
        android:textSize="20sp" />

    <TextView
        android:id="@+id/textView4"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_above="@+id/textView3"
        android:layout_alignStart="@+id/view"
        android:text="Reporter" />

    <TextView
        android:id="@+id/textView3"
        android:layout_width="239dp"
        android:layout_height="wrap_content"
        android:layout_alignBottom="@+id/view"
        android:layout_alignStart="@+id/view"
        android:text="Very quiet bespectacled reporter at Daily Planet. Clumsy fellow, but well built and looks strongly familiar. Wonder which celebrity he looks like." />

    <TextView
        android:id="@+id/textView12"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_alignParentTop="true"
        android:layout_alignStart="@+id/textView5"
        android:layout_marginTop="269dp"
        android:text="Bruce Wayne"
        android:textSize="20sp" />

    <TextView
        android:id="@+id/textView5"
        android:layout_width="wrap_content"
        android:layout_height="22dp"
        android:layout_above="@+id/textView6"
        android:layout_alignStart="@+id/textView6"
        android:text="Billionaire" />

    <TextView
        android:id="@+id/textView6"
        android:layout_width="250dp"
        android:layout_height="wrap_content"
        android:layout_alignBottom="@+id/view2"
        android:layout_toStartOf="@+id/imageView4"
        android:text="Incredibly smart billionaire industrialist and notorious playboy. Also very reclusive. Also most certainly the batman."
        android:textSize="14sp" />

    <TextView
        android:id="@+id/textView10"
        android:layout_width="165dp"
        android:layout_height="wrap_content"
        android:layout_alignStart="@+id/view"
        android:layout_alignTop="@+id/view3"
        android:text="Barrack Obama"
        android:textSize="20sp" />

    <TextView
        android:id="@+id/textView8"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_alignParentBottom="true"
        android:layout_alignStart="@+id/view"
        android:layout_marginBottom="177dp"
        android:text="President of the USA" />

    <TextView
        android:id="@+id/textView9"
        android:layout_width="243dp"
        android:layout_height="wrap_content"
        android:layout_alignBottom="@+id/view3"
        android:layout_alignStart="@+id/view"
        android:text="44th President of the USA. Recipient of the Nobel Prize (although no one is quite certain what exactly for. Suspected of being Black." />

    <ImageView
        android:id="@+id/imageView4"
        android:layout_width="68dp"
        android:layout_height="75dp"
        android:layout_alignEnd="@+id/view"
        android:layout_alignTop="@+id/view"
        app:srcCompat="@drawable/clarkkent" />

    <ImageView
        android:id="@+id/imageView5"
        android:layout_width="53dp"
        android:layout_height="75dp"
        android:layout_alignTop="@+id/textView12"
        android:layout_toEndOf="@+id/textView6"
        app:srcCompat="@drawable/brucewayne" />

    <ImageView
        android:id="@+id/imageView6"
        android:layout_width="61dp"
        android:layout_height="70dp"
        android:layout_alignEnd="@+id/imageView4"
        android:layout_alignTop="@+id/view3"
        app:srcCompat="@drawable/barackobama" />

    <Button
        android:id="@+id/btnActivity2"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_alignEnd="@+id/textView12"
        android:layout_alignTop="@+id/MapsActivity1"
        android:onClick="process"
        android:text="Next" />

</RelativeLayout>